# Aries Agent configurations
AGENT_SDK_REST_IMAGE=ghcr.io/trustbloc-cicd/agent-sdk-server
AGENT_SDK_REST_IMAGE_TAG=0.1.8-snapshot-e983f5d

# Webhook configurations
MOCK_WEBHOOK_IMAGE=ghcr.io/trustbloc/mock-webhook
MOCK_WEBHOOK_IMAGE_TAG=latest

# configs
HTTP_SCHEME=http
WS_SCHEME=ws
TRANSPORT_RETURN_OPTION_ALL=all

# default key type
AGENT_KEY_TYPE=ecdsap256ieee1363
AGENT_KEY_AGREEMENT_TYPE=p256kw

AGENT_MEDIA_TYPE_PROFILES=didcomm/aip2;env=rfc587,didcomm/v2

# Wallet Agent configurations
WALLET_AGENT_HOST=0.0.0.0
WALLET_AGENT_API_PORT=10210
WALLET_WEBHOOK_PORT=10211

# Adapter Agent configurations
ADAPTER_AGENT_HOST=0.0.0.0
ADAPTER_AGENT_API_PORT=10220
ADAPTER_AGENT_DIDCOMM_HTTP_PORT=10221
